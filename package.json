{
  "name": "frontend-server",
  "author": "Ba Toan Nguyen",
  "version": "2.0.0",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/frontend-server.js",
  "files": [
    "src",
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "test": "vitest --run",
    "tag:create": "node scripts/create-tag.js",
    "tag:patch": "npm run tag:create patch",
    "tag:minor": "npm run tag:create minor",
    "tag:major": "npm run tag:create major"
  },
  "license": "ISC",
  "peerDependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "4.21.2",
    "http-proxy-middleware": "^2.0.9",
    "mime-types": "^2.1.35"
  },
  "peerDependenciesMeta": {
    "http-proxy-middleware": {
      "optional": true
    }
  },
  "devDependencies": {
    "@chrisneedham/rollup-plugin-peer-deps-external": "^2.3.1",
    "@eslint/js": "^9.29.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cors": "^2.8.14",
    "@types/express": "4.17.21",
    "@types/http-proxy": "^1.17.12",
    "@types/mime-types": "^2.1.1",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "globals": "^16.2.0",
    "prettier": "^2.8.8",
    "semver": "^7.7.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.34.1",
    "vite": "6.2.2",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-static-copy": "2.3.0",
    "vitest": "3.0.9"
  },
  "engines": {
    "node": "20",
    "pnpm": "10"
  }
}
